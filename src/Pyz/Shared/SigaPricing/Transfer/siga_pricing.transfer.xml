<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CustomerDiscounts">
        <property name="onlineDiscount" type="int"/>
        <property name="onlineDiscountPercents" type="int"/>
        <property name="customerVolumeDiscount" type="int"/>
        <property name="customerVolumeDiscountPercents" type="int"/>
        <property name="customerVolumeDiscountNextLevelAmount" type="int"/>
        <property name="customerVolumeDiscountNextLevelPercents" type="int"/>
    </transfer>

    <transfer name="Totals">
        <property name="customerDiscountTotals" type="CustomerDiscounts"/>
    </transfer>

</transfers>
